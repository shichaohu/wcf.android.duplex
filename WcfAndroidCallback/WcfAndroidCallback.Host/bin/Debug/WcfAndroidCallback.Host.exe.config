<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors >
        <behavior name="serviceBehavior">
          <serviceDebug includeExceptionDetailInFaults="true" />
          <serviceMetadata httpGetEnabled="True" httpGetUrl="Http://localhost:10000/" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service name="WcfAndroidCallback.Service.AndroidService" behaviorConfiguration="serviceBehavior" >
        <endpoint address="AndroidService"
                  binding="netTcpBinding" contract="WcfAndroidCallback.Service.Interface.IAndroidService"></endpoint>

        <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange" />
        
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:10000/AndroidService/"/>
            <add baseAddress="net.tcp://localhost:10001/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
</configuration>